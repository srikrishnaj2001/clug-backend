{
  "name": "lms-be",
  "version": "1.0.0",
  "description": "Learning Management System Backend",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "npm run kill:port || true && PORT=8008 nodemon src/server.js",
    "test": "jest",
    "kill:port": "sudo kill -9 $(sudo lsof -t -i:8008)",
    "dev:clean": "npm run kill:port || true && npm run dev",
    "migrate": "sequelize-cli db:migrate",
    "db:migrate": "sequelize-cli db:migrate",
    "db:migrate:undo": "sequelize-cli db:migrate:undo",
    "db:migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "db:create": "sequelize-cli db:create",
    "db:drop": "sequelize-cli db:drop",
    "db:reset": "npm run db:drop && npm run db:create && npm run db:migrate && npm run seed",
    "db:reset:migrations": "npm run db:migrate:undo:all && npm run db:migrate",
    "seeders": "node src/scripts/seed.js",
    "fetch-transcripts": "node src/scripts/fetch-transcripts.js",
    "fetch-transcripts-simple": "node src/scripts/fetch-transcripts-simple.js",
    "create-transcript-placeholders": "node src/scripts/create-transcript-placeholders.js",
    "test-ai-transcript": "node src/scripts/test-ai-transcript.js"
  },
  "author": "",
  "license": "ISC",
  "keywords": [],
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "axios": "^1.9.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "dotenv-flow": "^4.1.0",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "pg": "^8.15.6",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.1",
    "uuid": "^11.1.0",
    "youtube-transcript": "^1.2.1",
    "youtubei.js": "^15.0.1"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "sequelize-cli": "^6.6.2"
  }
}
